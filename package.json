    "svg-transform-loader": "^2.0.13",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^3.9.7",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/~.*"
    ],
    "coverageDirectory": "test_results/jest",
    "coverageReporters": [
      "clover",
      "lcov",
      "text-summary"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tests/tsconfig.json",
        "babelConfig": false
      }
    },
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json",
      "tsx",
      "ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/file-mock.ts",
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/style-mock.ts",
      "^~/(.*)$": "<rootDir>/src/$1",
      "^react$": "preact/compat",
      "^react-dom/test-utils$": "preact/test-utils",
      "^react-dom$": "preact/compat"
    },
    "preset": "ts-jest",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "./test_results/jest",
          "outputName": "junit.xml",
          "ancestorSeparator": " > ",
          "usePathForSuiteName": "false"
        }
      ]
    ],
    "setupFiles": [
      "core-js/stable",
      "regenerator-runtime/runtime",
      "jest-localstorage-mock",
      "<rootDir>/tests/unhandled-promise-rejection.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ],
    "testEnvironment": "jest-environment-jsdom-global",
    "testMatch": null,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!vue)"
    ]
  }
}

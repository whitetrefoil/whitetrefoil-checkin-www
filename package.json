{
  "name": "whitetrefoil-checkin-www",
  "version": "0.1.0",
  "author": {
    "name": "WhiteTrefoil",
    "email": "whitetrefoil@gmail.com"
  },
  "scripts": {
    "build": "gulp build",
    "start": "gulp serve -d",
    "serve": "gulp serve -d",
    "lint": "rimraf test_results/eslint && eslint --ext .js,.jsx,.ts,.tsx -o test_results/eslint/junit.xml -f junit src",
    "test": "rimraf test_results/jest && jest --no-cache --no-coverage",
    "coverage": "rimraf test_results/jest && jest --no-cache"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.0",
    "@types/classnames": "^2.2.10",
    "@types/core-js": "^2.5.3",
    "@types/object.omit": "^3.0.0",
    "@types/react-transition-group": "^4.4.0",
    "@types/shortid": "^0.0.29",
    "@whitetrefoil/jsx-sp-events": "^0.1.0",
    "@whitetrefoil/log-utils": "^0.2.1",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "ky": "^0.20.0",
    "object.omit": "^3.0.0",
    "preact": "^10.4.4",
    "react-transition-group": "^4.4.1",
    "regenerator-runtime": "^0.13.5",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-do-expressions": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^10.0.4",
    "@types/cssnano": "^4.0.0",
    "@types/fancy-log": "^1.3.1",
    "@types/fs-extra": "^9.0.1",
    "@types/gulp": "^4.0.6",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/jest": "^25.2.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/meow": "^5.0.0",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^14.0.5",
    "@types/testing-library__jest-dom": "^5.7.0",
    "@types/testing-library__react": "^10.0.1",
    "@types/webpack": "^4.41.13",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "@types/webpack-dev-server": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "@whitetrefoil/koa-http-proxy": "^1.0.0",
    "@whitetrefoil/msm": "^0.9.0",
    "autoprefixer": "^9.8.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "chalk": "^4.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "eslint": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "fancy-log": "^1.3.3",
    "fiber": "^1.0.4",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.4",
    "gulp": "^4.0.2",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "jest-environment-jsdom": "^26.0.1",
    "jest-environment-jsdom-global": "^2.0.2",
    "jest-junit": "^10.0.0",
    "jest-localstorage-mock": "^2.4.2",
    "koa-bodyparser": "^4.3.0",
    "meow": "^7.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^4.0.1",
    "resolve-url-loader": "^3.1.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "svg-transform-loader": "^2.0.13",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.9.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/~.*"
    ],
    "coverageDirectory": "test_results/jest",
    "coverageReporters": [
      "clover",
      "lcov",
      "text-summary"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tests/tsconfig.json",
        "babelConfig": false
      }
    },
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json",
      "tsx",
      "ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/file-mock.ts",
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/style-mock.ts",
      "^~/(.*)$": "<rootDir>/src/$1",
      "^react$": "preact/compat",
      "^react-dom/test-utils$": "preact/test-utils",
      "^react-dom$": "preact/compat"
    },
    "preset": "ts-jest",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "./test_results/jest",
          "outputName": "junit.xml",
          "ancestorSeparator": " > ",
          "usePathForSuiteName": "false"
        }
      ]
    ],
    "setupFiles": [
      "core-js/stable",
      "regenerator-runtime/runtime",
      "jest-localstorage-mock",
      "<rootDir>/tests/unhandled-promise-rejection.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ],
    "testEnvironment": "jest-environment-jsdom-global",
    "testMatch": null,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!vue)"
    ]
  }
}
